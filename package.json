    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "postgrator-cli": "^4.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
